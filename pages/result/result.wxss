.info {
    text-align: center;
    padding: 70rpx 20rpx;
}

.scoreBox {
    position: relative;
    border-radius: 20rpx;
    border: 15rpx solid #86ba3a;
}

.jzImg {
    position: absolute;
    width: 106rpx;
    height: 58rpx;
    left: 50%;
    top: -58rpx;
    margin-left: -53rpx;
}

.username {
    font-size: 44rpx;
    color: #6e8e1a;
}

.scoreData {
    display: inline-block;
    position: relative;
    color: #86ba3a;
    font-size: 62rpx;
    text-align: center;
}

.circle {
    padding: 15rpx;
    width: 126rpx;
    height: 133rpx;
}

.score {
    position: absolute;
    width: 100rpx;
    left: 32rpx;
    top: 38rpx;
    text-align: center;
    font-weight: bold;
}

.tit {
    color: #333;
    font-size: 32rpx;
}

.hintBox {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-flow: wrap row;
    padding: 20rpx 0 10rpx;
}

.laba {
    width: 77rpx;
    height: 77rpx;
}

.hintList {
    display: flex;
}

.point {
    vertical-align: top;
    margin-top: 14rpx;
    width: 12rpx;
    height: 12rpx;
}

.hintText {
    display: inline-block;
    vertical-align: top;
    max-width: 500rpx;
    padding-left: 12rpx;
    font-size: 24rpx;
    line-height: 36rpx;
    color: #818181;
}

.hintRight {
    padding-left: 20rpx;
    text-align: left;
}

.titBar {
    text-align: center;
    position: relative;
}

.titName {
    font-size: 34rpx;
    color: #fff;
    line-height: 58rpx;
}

.titBg {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 58rpx;
    z-index: -1;
}

.bgBox {
    padding: 30rpx 20rpx 35rpx;
}


.bgItem {
    display: flex;
    margin-bottom: 22rpx;
    flex-flow: wrap row;
    justify-content: space-between;
}

.tagInfo {
    width: 580rpx;
    color: #818181;
    font-size: 26rpx;
}

.tag {
    text-align: center;
    font-size: 26rpx;
    width: 110rpx;
    height: 38rpx;
    line-height: 38rpx;
    border-radius: 6rpx;
    border: 1rpx solid transparent;
}

.tagNice {
    border-color: #32ba2f;
}

.tagBad {
    border-color: #ed3333;
}

.green {
    color: #32ba2f;
}
.green2{
  color: #6e8e1a
}

.red {
    color: #ed3333;
}

.tagTit {
    color: #000;
    font-weight: bold;
}

.exportBox {
    padding: 30rpx 0 35rpx;
    background: #f3f3f3;
}

.resnUum {
    text-align: center;
}

.resTit {
    color: #6e8e1a;
    font-size: 36rpx;
}

.resTit2 {
    color: #777;
    font-size: 30rpx;
    margin-bottom: 30rpx;
}

.resData {
    padding: 15rpx 0;
    font-size: 72rpx;
}

.handle {
    display: flex;
    flex-flow: wrap row;
    justify-content: space-around;
}

.handleBtn {
    color: #6e8e1a;
    font-size: 30rpx;
    width: 220rpx;
    height: 54rpx;
    line-height: 54rpx;
    border: 1rpx solid #98bc36;
}

.tapBtn {
    background: #6e8e1a;
    color: #fff;
}

.schoolBox {
    padding: 10rpx 20rpx 0;
}

.schoolItem {
    display: flex;
    flex-flow: row wrap;
    align-items: center;
    padding: 22rpx 0;
    justify-content: space-between;
    border-bottom: 1rpx solid #d6d6d6;
}

.scImg {
    width: 130rpx;
    height: 120rpx;
    margin-right: 15rpx;
}

.scInfo {
    width: 560rpx;
    text-align: left;
}

.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.chName {
    font-family: 'Microsoft Yahei';
    color: #3c3b3b;
    font-size: 32rpx;
}

.enName {
    padding: 13rpx;
    color: #848484;
    font-size: 24rpx;
}

.addres {
    font-size: 22rpx;
    color: #5a5a5a;
}

.gps {
    width: 18rpx;
    height: 22rpx;
    margin-right: 14rpx;
}