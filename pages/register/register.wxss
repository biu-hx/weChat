.register {
    width: 90%;
    margin: 60rpx auto;
}

button::after {
    border: none;
}

.reg-input {
    width: 100%;
    margin-bottom: 20rpx;
    display: flex;
    font-size: 24rpx;
}

.reg-input input {
    width: 100%;
    height: 74rpx;
    background-color: #f1f1f1;
    padding-left: 40rpx;
    box-sizing: border-box;
}

.reg-input button {
    width: 35%;
    height: 74rpx;
    line-height: 74rpx;
    margin-left: 5%;
    font-size: 24rpx;
    background-color: #f1c100;
    color: white;
    border: none;
    border-radius: 0;
}

.reg-btn {
    width: 90%;
    height: 82rpx;
    line-height: 82rpx;
    border-radius: 60rpx;
    color: white;
    font-size: 30rpx;
    background-color: #769f05;
    margin-top: 60rpx;
}

.bot-font {
    width: 100%;
    margin-top: 40rpx;
    display: flex;
    justify-content: center;
}

.bot-font text {
    font-size: 20rpx;
    color: #767676;
}

.bot-font navigator {
    color: #729c00;
    font-size: 20rpx;
}